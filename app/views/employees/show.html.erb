<h2>Employee Information:</h2>
<h4><img src="<%=@employee.img_url%>" alt="Employee Image"></h4>
<ul>
    <li><h4><%= @employee.first_name + " " + @employee.last_name%></h4></li>
    <li><h4><%= @employee.alias%></h4></li>
    <li><h4><%= @employee.title%></h4></li>
    <li><h4><%= @employee.office%></h4></li>
</ul>


